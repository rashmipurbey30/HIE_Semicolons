@model IEnumerable<VirtualHIE.Models.PatientDataAccess>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AccessRequestDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsLatest)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hospital.HospitalName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Patient.AadharId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AccessRequestDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsLatest)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Hospital.HospitalName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Patient.AadharId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
